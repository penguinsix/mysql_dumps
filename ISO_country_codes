CREATE TABLE `regions` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `iso` varchar(45) DEFAULT NULL,
  `iso3` varchar(45) DEFAULT NULL,
  `fips` varchar(45) DEFAULT NULL,
  `country` varchar(45) DEFAULT NULL,
  `continent` varchar(45) DEFAULT NULL,
  `currency_code` varchar(45) DEFAULT NULL,
  `currency_name` varchar(45) DEFAULT NULL,
  `phone_prefix` varchar(45) DEFAULT NULL,
  `postal_code` varchar(45) DEFAULT NULL,
  `languages` varchar(45) DEFAULT NULL,
  `geonameid` varchar(45) DEFAULT NULL,
  `country_en` varchar(70) NOT NULL,
  `country_jp` varchar(70) NOT NULL,
  `country_zh` varchar(70) NOT NULL,
  `country_ko` varchar(70) NOT NULL,
  `country_fr` varchar(70) NOT NULL,
  `country_de` varchar(70) NOT NULL,
  `country_es` varchar(70) NOT NULL,
  `country_ru` varchar(70) NOT NULL,
  `country_it` varchar(70) NOT NULL,
  `country_pt` varchar(70) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

LOCK TABLES `regions` WRITE;
